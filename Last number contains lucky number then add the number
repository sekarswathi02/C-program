Last number contains lucky number then add the number.
#include <stdio.h>
int main() {
    int n, luckyDigit, sum = 0;
    printf("Enter your lucky digit: ");
    scanf("%d", &luckyDigit);
    printf("Enter the size of the array: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter %d elements of the array:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if (arr[i] % 10 == luckyDigit) {
            sum += arr[i];
        }
    }
    printf("Sum of numbers ending with the lucky digit %d: %d\n", luckyDigit, sum);
    return 0;
}
#include <stdio.h>
int main() {
    int n, luckyDigit, sum = 0;
    printf("Enter your lucky digit: ");
    scanf("%d", &luckyDigit);
    printf("Enter the size of the array: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter %d elements of the array:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if (arr[i] % 10 == luckyDigit) {
            sum += arr[i];
        }
    }
    printf("Sum of numbers ending with the lucky digit %d: %d\n", luckyDigit, sum);
    return 0;
}
