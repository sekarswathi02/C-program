#include <stdio.h>
#include <math.h>
void main()
{
   int a,i,temp,r,sum=0;
   scanf("%d",&a);
   temp=a;
   while(a!=0)
   {
       r=a%10;
       sum=sum+pow(r,3);
       a=a/10;
   }   
       if(sum==temp)
       {
           printf("Amstrong");
       }
       else
       {
           printf("Not amstrong");
       }
   
}
