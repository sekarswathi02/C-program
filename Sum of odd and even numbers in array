#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int sum1,sum2=0;
    int arr[n];
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
    for(int i=0;i<n;i++)
    {
        if(arr[i]%2==0)
        {
            sum1+=arr[i];
        }
        else
        {
            sum2+=arr[i];
        }
    }
    printf("Sum 1 =%d\n",sum1);
    printf("Sum 2 =%d",sum2);
}
