#include <stdio.h>
int main() {
    char c[100];
    int i,count = 0;
    scanf("%[^\n]s", c);
    while(c[i] != '\0') {
        if (c[i] == 'a' || c[i] == 'e' || c[i] == 'i' || c[i] == 'o' || c[i] == 'u' || 
            c[i] == 'A' || c[i] == 'E' || c[i] == 'I' || c[i] == 'O' || c[i] == 'U') {
            count++;
        }
        i++;
    }
    printf("%d\n", count);
    return 0;
