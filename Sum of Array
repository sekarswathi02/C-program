#include <stdio.h>

void main() {
    
    printf("Enter the size: ");
    int n;
    scanf("%d", &n);
    int arr[n];
    int sum=0;
    printf("Enter %d elements:",n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        
        sum=sum+arr[i];
    }
    printf("sum=%d",sum);
}
